{
    "name": "Blonde",
    "version": "0.0.1",
    "description": "Blonde, A simple theme using Tailwind CSS.",
    "main": "index.js",
    "repository": "https://github.com/opera7133/Blonde",
    "author": "wamo <ainznino@pm.me>",
    "license": "MIT",
    "scripts": {
      "dev": "hugo server --gc --disableFastRender",
      "dev:drafts": "hugo server --gc --disableFastRender --buildFuture --buildDrafts",
      "build": "hugo --gc --cleanDestinationDir --minify",
      "start:dev": "hugo server --gc --disableFastRender --renderToDisk  --cleanDestinationDir",
      "start:prod": "hugo server --gc --disableFastRender --renderToDisk  --cleanDestinationDir --minify --environment production",
      "npm-check": "npm-check -u"
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^4.0.3",
        "@tailwindcss/typography": "^0.4.1",
        "autoprefixer": "^10.3.7",
        "postcss": "^8.3.9",
        "postcss-cli": "^9.0.1",
        "postcss-import": "^14.0.2",
        "tailwindcss": "^2.2.16"
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "maintained node versions",
        "not dead"
    ]
}
